            <div id="footer" data-ng-controller="Header_FooterController" data-ng-init="GetHeaderContent3()">
              <span  data-ng-bind-html="HeaderList3[14].HeaderContent"></span> <%-- © 2009 MyCompany--%> | <a href="Terms.aspx"  data-ng-bind-html="HeaderList3[15].HeaderContent"><%--Terms of Service--%> </a> | <a href="Privacy.aspx" data-ng-bind-html="HeaderList3[16].HeaderContent"> <%--Privacy Policy--%></a>
            </div>
            <!-- End Footer -->
        </div>
    </form>

    <script type="text/javascript" src="js/jquery-1.8.3.min.js"></script>
    <script src="Scripts/angular.js"></script>
    <script src="Scripts/angular-sanitize.min.js"></script>
    <script src="AgController/HeaderFooterJS.js"></script>
        <!-- End Wrapper -->
</body>
</html>