<body  data-ng-app="myApp1">
    <form name="aspnetForm" runat="server">
        <div id="wrapper">
          <div data-ng-controller="HeaderFooterController" data-ng-init="GetHeaderContent()">
               <div id="headernav">
                <div id="toptag">
                    <span id="ctl00_lblWelcome" style="font-weight: bold;"></span>
                </div>
                <div id="topnav">
                    <div class="l">
                    </div>
                    <div class="m">
                        <div style="overflow: hidden; float: left;">
                            <ul class="rtsUL">
                                <li class="rtsLI"><a class="rtsLink" href="Default.aspx"><span class="rtsOut"><span
                                    class="rtsIn"><span class="rtsTxt">
                                        <span id="ctl00_lblHome" data-ng-bind-html="HeaderList[0].HeaderContent"></span></span></span></span></a></li>
                                <li class="rtsLI"><a class="rtsLink" href="Services.aspx"><span class="rtsOut"><span
                                    class="rtsIn"><span class="rtsTxt">
                                        <span id="ctl00_lblOurService"><div data-ng-bind-html="HeaderList[1].HeaderContent"></div></span></span></span></span></a></li>
                                <li class="rtsLI"><a class="rtsLink" href="AboutUs.aspx"><span class="rtsOut"><span
                                    class="rtsIn"><span class="rtsTxt">
                                        <span id="ctl00_lblAboutUs" data-ng-bind-html="HeaderList[2].HeaderContent"></span></span></span></span></a></li>
                                <li class="rtsLI"><a class="rtsLink" href="ContactUs.aspx"><span class="rtsOut"><span
                                    class="rtsIn"><span class="rtsTxt">
                                        <span id="ctl00_lblContactUs" data-ng-bind-html="HeaderList[3].HeaderContent"><%--Contact Us--%></span></span></span></span></a></li>
                            </ul>
                        </div>
                        <div style="float: left; margin-left: 20px;">
                        </div>
                    </div>
                    <div class="r">
                    </div>
                </div>
                <div id="smallnav">
                    <div id="ctl00_langBar1_RadTabStrip1" class="RadTabStrip RadTabStrip_Test RadTabStripTop_Test ">
                        <!-- 2008.3.1314.20 -->
                        <div class="rtsLevel rtsLevel1">
                            <ul class="rtsUL">
                                <li class="rtsLI"><a class="rtsLink" href="basket.aspx"><span class="rtsOut"><span
                                    class="rtsIn"><span class="rtsTxt">
                                        <span id="ctl00_lbViewCart" data-ng-bind-html="HeaderList[4].HeaderContent"><%--View Cart--%></span></span></span></span></a></li>
                                <li>
                                    <a onclick="return openLogin();" id="ctl00_lbLogin" navigateurl="SD_AccountSummary.aspx" href="javascript:__doPostBack(&#39;ctl00$lbLogin&#39;,&#39;&#39;)"  data-ng-bind-html="HeaderList[6].HeaderContent"><%--My Account--%></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Begin Header -->
            <div id="header">
                <div class="logo">
                    <h1 class="lineone"  data-ng-bind-html="HeaderList[8].HeaderContent">
                      <%--  <span>My</span> Company--%></h1>
                    <h2 class="linetwo"  data-ng-bind-html="HeaderList[9].HeaderContent"><%--"Your online Phone"--%></h2>
                </div>
            </div>
            <!-- End Header -->
            <!-- Begin Navigation -->
            <div id="leftnavigation">
            </div>
            <div id="navigation">
                <div id="ctl00_rtsMain" class="RadTabStrip RadTabStrip_Test RadTabStripTop_Test" style="font-size: Medium;">
                    <!-- 2010.2.929.35 -->
                    <div class="rtsLevel rtsLevel1">
                        <ul class="rtsUL">
                            <li class="rtsLI rtsFirst"><a class="rtsLink" href="SelectHsService.aspx"><span class="rtsOut"><span class="rtsIn"><span class="rtsTxt"  data-ng-bind-html="HeaderList[10].HeaderContent"><%--Home Service--%></span></span></span></a></li>
                            <li class="rtsLI"><a class="rtsLink" href="Cards.aspx"><span class="rtsOut"><span class="rtsIn"><span class="rtsTxt" data-ng-bind-html="HeaderList[11].HeaderContent"><%--Calling Card--%></span></span></span></a></li>
                            <li class="rtsLI"><a class="rtsLink" href="Plans.aspx?Type=1"><span class="rtsOut"><span class="rtsIn"><span class="rtsTxt"  data-ng-bind-html="HeaderList[12].HeaderContent"><%--Residential--%></span></span></span></a></li>
                            <li class="rtsLI rtsLast"><a class="rtsLink" href="InternationalTopup.aspx"><span class="rtsOut"><span class="rtsIn"><span class="rtsTxt" data-ng-bind-html="HeaderList[13].HeaderContent"><%--International Topup--%></span></span></span></a></li>
                        </ul>
                    </div>
                    <input id="ctl00_rtsMain_ClientState" name="ctl00_rtsMain_ClientState" type="hidden" />
                </div>
            </div>
            <!-- End Navigation -->
            <div id="rightnavigation">
            </div>
            </div>
            <!-- Begin Top header -->
            <asp:ContentPlaceHolder ID="MainContent" runat="server">
            </asp:ContentPlaceHolder>
          
            <!-- End Right Column -->
